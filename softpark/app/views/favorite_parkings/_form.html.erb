<%= simple_form_for(@favorite_parking) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :fec_favorite %>
    <%= f.input :name_space %>
    <%= f.association :user %>
    <%= f.association :rent_parking %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
