<%= simple_form_for(@rent_parking) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :price %>
    <%= f.input :addres %>
    <%= f.input :phone %>
    <%= f.input :location,:collection => @locations,:label_method => :desDistrict,:value_method => :id,:label => "Location" ,:include_blank => false %>
    <%= f.input :type_parking,:collection => @type_parkings,:label_method => :description,:value_method => :id,:label => "Type parking" ,:include_blank => false %>

    
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
